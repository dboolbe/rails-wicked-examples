<h1>Listing my_models</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Suffix</th>
      <th>Twitter username</th>
      <th>Github username</th>
      <th>Website</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Street</th>
      <th>Car year</th>
      <th>Car maker</th>
      <th>Car model</th>
      <th>Car type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @my_models.each do |my_model| %>
      <tr>
        <td><%= my_model.first_name %></td>
        <td><%= my_model.middle_name %></td>
        <td><%= my_model.last_name %></td>
        <td><%= my_model.suffix %></td>
        <td><%= my_model.twitter_username %></td>
        <td><%= my_model.github_username %></td>
        <td><%= my_model.website %></td>
        <td><%= my_model.city %></td>
        <td><%= my_model.state %></td>
        <td><%= my_model.zipcode %></td>
        <td><%= my_model.street %></td>
        <td><%= my_model.car_year %></td>
        <td><%= my_model.car_maker %></td>
        <td><%= my_model.car_model %></td>
        <td><%= my_model.car_type %></td>
        <td><%= link_to 'Show', my_model %></td>
        <td><%= link_to 'Edit', edit_my_model_path(my_model) %></td>
        <td><%= link_to 'Destroy', my_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New MyModel via Basic Form', new_my_model_path %><br />
<%= link_to 'New MyModel via Wicked Wizard', wicked_index_path, method: :post %><br />
<%= link_to 'New MyModel via Wicked Wizard passing params via GET and storing model only once', new_wicked_using_get_path %>
